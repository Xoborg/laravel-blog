{
  "name": "xoborg/laravel-blog",
  "description": "A simple blog package for Laravel",
  "keywords": [
	"xoborg",
	"laravel",
	"blog"
  ],
  "homepage": "https://github.com/Xoborg/laravel-blog",
  "license": "MIT",
  "authors": [
	{
	  "name": "Carlos Rodr√≠guez",
	  "email": "carlosrodriguez@xoborg.com",
	  "homepage": "http://xoborg.com",
	  "role": "Developer"
	}
  ],
  "require": {
    "php": "^7.1",
    "illuminate/auth": "5.6.*|5.7.*",
    "illuminate/console": "5.6.*|5.7.*",
    "illuminate/database": "5.6.*|5.7.*",
    "illuminate/http": "5.6.*|5.7.*",
    "illuminate/routing": "5.6.*|5.7.*",
    "illuminate/support": "5.6.*|5.7.*",
    "illuminate/validation": "5.6.*|5.7.*",
    "spatie/laravel-feed": "^2.1"
  },
  "require-dev": {
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "~3.6.0|~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
	"psr-4": {
	  "Xoborg\\LaravelBlog\\": "src"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "Xoborg\\LaravelBlog\\Tests\\": "tests"
	}
  },
  "scripts": {
	"test": "vendor/bin/phpunit"
  },
  "config": {
	"sort-packages": true
  },
  "extra": {
	"laravel": {
	  "providers": [
		"Xoborg\\LaravelBlog\\LaravelBlogServiceProvider"
	  ]
	}
  }
}
